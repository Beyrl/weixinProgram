<!--pages/tip-2-2/tip-2-2.wxml-->
<view class="section-wrapper">
  <text class="section-title">下面是效果</text>
  <view class="section-detail" >{{desc}}</view>
  <view class="section-content">
    <view class="section-main main2">
      <switch wx:for="{{objectArray}}" wx:key="unique" > {{item.id}} </switch>
      <button class="button"  bindtap="switch"> Switch </button>
      <button class="button"  bindtap="addToFront"> Add to the front </button>
    </view>
    <view class="section-main main2">
      <switch wx:for="{{numberArray}}" wx:key="*this" > {{item}} </switch>
      <button class="button"  bindtap="addNumberToFront"> Add to the front </button>
    </view>
    <image src="../../assets/image/tip2-2-1.png"></image>
    <image src="../../assets/image/tip2-2-2.png"></image>
  </view>
</view>

